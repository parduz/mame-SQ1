		<disk state="0" statemask="0x08"> <bounds x="3.3" /> <color red="0.125" green="0.125" blue="0.125" /> </disk>
		<disk state="0" statemask="0x10"> <bounds x="4.4" /> <color red="0.125" green="0.125" blue="0.125" /> </disk>
		<disk           statemask="0x01"> <bounds x="0.0" /> </disk>
		<disk           statemask="0x02"> <bounds x="1.1" /> </disk>
		<disk           statemask="0x04"> <bounds x="2.2" /> </disk>
		<disk           statemask="0x08"> <bounds x="3.3" /> </disk>
		<disk           statemask="0x10"> <bounds x="4.4" /> </disk>
	</element>
	<!-- 2x16 LCD display -->
	<group name="basic2x16LCDpage">
		<repeat count="2">
			<param name="origin" start="~start~" increment="112" />
			<param name="row" start="0" increment="10" />
			<repeat count="16">
				<param name="char" start="~origin~" increment="7" />
				<param name="x" start="0" increment="6" />
				<repeat count="7">
					<param name="n" start="~char~" increment="1" />
					<param name="y" start="~row~" increment="1" />
					<element name="pg_~n~" ref="dotrow" state="0">
						<bounds x="~x~" y="~y~" width="5" height="1" />
					</element>
				</repeat>
			</repeat>
		</repeat>
	</group>
