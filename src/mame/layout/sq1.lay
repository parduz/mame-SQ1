	<!-- LCD character -->
	<element name="dotrow">
		<rect>            <bounds x="-0.05" width="5.55" y="-0.05" height="1.1" /> <color red="0.0" green="0.0" blue="0.0" /> </rect>
		<disk state="0" statemask="0x01"> <bounds x="0.0" /> <color red="0.125" green="0.125" blue="0.125" /> </disk>
